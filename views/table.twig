<table>
  <tr>
    <th>Name</th>
    <th>Logo Small</th>
    <th>Logo Large</th>
    <th>Logo 120x120</th>
    <th>Logo 210x80</th>
    <th>Logo 450x450</th>
  </tr>


  {% for airline in airlines %}
    <tr>
      <td>{{ airline.name }}</td>
      <td><img src="{{airline.logo_small}}" class="invisble" /></td>
      <td><img src="{{airline.logo_large}}" class="invisble" /></td>
      <td><img src="{{airline.logo_120x120}}" class="invisble" /></td>
      <td><img src="{{airline.logo_210x80}}" class="invisble" /></td>
      <td><img src="{{airline.logo_450x450}}" class="invisble" /></td>
    </tr>
  {% endfor %}

</table>
